<templateSet group="user">
  <template name="fun" value="function() {&#10;   &#10;}" description="create lambda function" toReformat="true" toShortenFQNames="true">
    <context>
      <option name="PHP Expression" value="true" />
    </context>
  </template>
  <template name="rg" value="Route::get('')" description="" toReformat="true" toShortenFQNames="true">
    <context>
      <option name="PHP Expression" value="true" />
      <option name="PHP Statement" value="true" />
    </context>
  </template>
  <template name="au" value="    auth()-&gt;user()" description="auth user" toReformat="true" toShortenFQNames="true">
    <context>
      <option name="PHP Expression" value="true" />
    </context>
  </template>
  <template name="@index" value="public function index()&#10;    {&#10;        &#10;    }" description="Create index function" toReformat="true" toShortenFQNames="true">
    <context>
      <option name="PHP Class Member" value="true" />
    </context>
  </template>
  <template name="@" value="public function $NAME$()&#10;{&#10;    $END$&#10;}" description="" toReformat="true" toShortenFQNames="true">
    <variable name="NAME" expression="" defaultValue="" alwaysStopAt="true" />
    <context>
      <option name="PHP" value="true" />
    </context>
  </template>
</templateSet>